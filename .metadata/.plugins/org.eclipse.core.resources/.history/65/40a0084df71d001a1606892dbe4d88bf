package exmaple.com.tree;

import java.io.IOException;

class AirPlain {
	public AirPlain()throws IOException{
		System.out.println("Air plain");
		throw new IOException();
	}
 }

class AirJet extends AirPlain{
	
}
public class Test{
	public static void main(String[] args) {
		try {
			new AirJet();
		}
		catch(IOException e) {
			System.out.println("IOException is trown");
		}
	}
}