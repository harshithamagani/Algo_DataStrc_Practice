package exmaple.com;

public class CheckIfNumAreSeqJava {

	public boolean solution(int [] arr, int k) {
	    //int n = arr.length;
	    for(int i=0;i<arr.length;i++) {
	    	if(arr[i] >arr[i+1]) {
	    		 return false;
	    	}
	    }
	    return true;
	}
	public static void main(String[] args) {
		
	}
}
